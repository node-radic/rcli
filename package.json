    "@types/tmp": "^0.0.33",
    "@types/fs-extra": "^3.0.0",
    "@types/globule": "^1.1.1",
    "@types/gulp": "^3.8.32",
    "@types/gulp-util": "^3.0.31",
    "@types/inquirer": "^0.0.32",
    "@types/jasmine": "^2.2.33",
    "@types/moment": "^2.13.0",
    "@types/node": "^7.0.8",
    "@types/reflect-metadata": "^0.0.5",
    "@types/string": "^0.0.28",
    "@types/through2": "^2.0.32",
    "@types/vinyl": "^2.0.0",
    "@types/vinyl-fs": "^2.4.5",
    "@types/winston": "^2.3.0",
    "bluebird": "^3.4.1",
    "browserify": "^13.0.0",
    "chai": "^3.4.1",
    "connect": "^3.5.0",
    "es6-symbol": "^3.1.0",
