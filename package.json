{
  "name": "rcli",
  "version": "0.0.1",
  "description": "Console ",
  "main": "src/index.js",
  "typings": "lib/index.d.ts",
  "jsnext:main": "src/index.js",
  "author": "Robin Radic",
  "target": "es5",
  "module": "comonjs",
  "bin": {
    "r": "bin/r.js",
    "connect": "bin/connect.sh"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "license": "MIT",
  "repository": "https://github.com/node-radic/rcli",
  "scripts": {
    "build": "gulp build",
    "clean": "gulp clean",
    "dev:init": "npm run dev:install; gulp build; npm run dev:docs",
    "dev:install": "rm -rf node_modules; rm yarn.lock; yarn install",
    "dev:docs": "rm -r docs &> /dev/null ; typedoc --includeDeclarations --target ES6 --exclude spec example example2 node_modules types/lodash --ignoreCompilerErrors --mode file --excludeExternals --excludePrivate --readme docs.md --out docs ./lib/index.d.ts",
    "deploy:docs": "npm run dev:docs; gulp ghpages",
    "deploy:patch": "npm run test && mversion patch -m && npm run deploy:docs",
    "deploy:minor": "npm run test && mversion minor -m && npm run deploy:docs",
    "deploy:major": "npm run test && mversion major -m && npm run deploy:docs",
    "dev": "gulp dev --cwd . && karma start",
    "dev:ci": "gulp dev:ci --cwd . && karma start --single-run",
    "dev:ci:phantomjs": "gulp dev:ci --cwd . && karma start --browsers PhantomJS --single-run",
    "test": "npm run build && gulp test"
  },
  "dependencies": {
    "@radic/console": "0.0.8",
    "@radic/console-colors": "^0.9.5",
    "@radic/util": "^1.7.1",
    "@types/faker": "^4.1.0",
    "@types/moment": "^2.13.0",
    "cryptico": "^1.0.2",
    "cryptr": "^2.0.0",
    "faker": "^4.1.0",
    "moment": "^2.18.1",
    "pump": "^1.0.2"
  },
  "devDependencies": {
    "@types/archy": "^0.0.30",
    "@types/async": "^2.0.40",
    "@types/axios": "^0.14.0",
    "@types/commander": "^2.9.0",
    "@types/dotenv": "^4.0.0",
    "@types/fs-extra": "^3.0.0",
    "@types/globule": "^1.1.1",
    "@types/gulp": "^3.8.32",
    "@types/gulp-util": "^3.0.31",
    "@types/inquirer": "^0.0.32",
    "@types/jasmine": "^2.2.33",
    "@types/moment": "^2.13.0",
    "@types/node": "^7.0.8",
    "@types/reflect-metadata": "^0.0.5",
    "@types/shelljs": "^0.7.1",
    "@types/string": "^0.0.28",
    "@types/through2": "^2.0.32",
    "@types/tmp": "^0.0.33",
    "@types/vinyl": "^2.0.0",
    "@types/vinyl-fs": "^2.4.5",
    "@types/winston": "^2.3.0",
    "bluebird": "^3.4.1",
    "browserify": "^13.0.0",
    "chai": "^3.4.1",
    "cli-clear": "^1.0.4",
    "connect": "^3.5.0",
    "copy": "^0.3.0",
    "cryptico": "^1.0.2",
    "cryptr": "^2.0.0",
    "del": "^3.0.0",
    "es6-symbol": "^3.1.0",
    "globule": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-gh-pages": "^0.5.4",
    "gulp-header": "1.8.8",
    "gulp-istanbul": "^1.0.0",
    "gulp-jasmine": "^2.4.1",
    "gulp-mocha": "^3.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.5.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tsd": "0.1.1",
