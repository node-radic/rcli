{
    "name": "@radic/cli",
    "version": "0.0.4",
    "description": "cli utility for various purposes. uses radical-console to provide a hierarchical command structure.",
    "main": "lib/index.js",
    "typings": "dts/index.d.ts",
    "jsnext:main": "lib-es6/index.js",
    "author": "Robin Radic",
    "target": "es5",
    "module": "comonjs",
    "bin": {
        "r": "bin/r.js",
        "connect": "bin/connect.sh"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "src",
        "bin"
    ],
    "private": false,
    "license": "MIT",
    "repository": "https://bitbucket.org/robin-radic/rcli",
    "scripts": {
        "link": "npm link radical-console && npm link @radic/console-colors && npm link @radic/util",
        "reinstall": "rm -rf node_modules yarn.lock package-lock.json && yarn install",
        "start": "npm run reinstall && && npm run link && npm run build && npm run watch",
        "watch": "gulp --gulpfile gulpfile.ts watch",
        "build": "gulp --gulpfile gulpfile.ts build",
        "clean": "gulp --gulpfile gulpfile.ts clean",
        "compiler:init": "gulp --gulpfile gulpfile.ts compiler:init",
        "compiler:compile": "gulp --gulpfile gulpfile.ts compiler:compile",
        "dev:init": "npm run dev:install; gulp build; npm run dev:docs",
        "dev:install": "rm -rf node_modules; rm yarn.lock; yarn install",
        "dev:docs": "rm -r docs &> /dev/null ; typedoc --includeDeclarations --target ES6 --exclude spec example example2 node_modules types/lodash --ignoreCompilerErrors --mode file --excludeExternals --excludePrivate --readme docs.md --out docs ./lib/index.d.ts",
        "deploy:docs": "npm run dev:docs; gulp ghpages",
        "deploy:patch": "npm run test && mversion patch -m && npm run deploy:docs",
        "deploy:minor": "npm run test && mversion minor -m && npm run deploy:docs",
        "deploy:major": "npm run test && mversion major -m && npm run deploy:docs",
        "test": "npm run build && gulp --gulpfile gulpfile.ts test"
    },
    "dependencies": {
        "radical-console": "file:../../radical-console",
        "@radic/console-colors": "^0.9.6",
        "@radic/util": "^1.7.5",
        "reflect-metadata": "^0.1.3",
        "Validator": "^1.0.4",
        "cryptico": "^1.0.2",
        "crypto-js": "^3.1.9-1",
        "cryptr": "^2.0.0",
        "faker": "^4.1.0",
        "flat-cache": "^1.2.2",
        "image-to-ascii": "^3.0.7",
        "jira": "^0.9.2",
        "knex": "^0.13.0",
        "lokijs": "^1.5.0",
        "moment": "^2.18.1",
        "objection": "^0.7.12",
        "omelette": "^0.4.4",
        "open": "0.0.5",
        "open-in-editor": "^2.2.0",
        "pg": "^6.4.0",
        "pn": "^1.0.0",
        "pump": "^1.0.2",
        "raven": "^2.0.2",
        "raven-winston": "0.0.3",
        "semver": "^5.4.1",
        "sqlite": "^2.8.0",
        "svg2png": "^4.1.1",
        "winston-sentry": "^0.2.1"
    },
    "devDependencies": {
        "@types/crypto-js": "^3.1.33",
        "@types/semver": "^5.3.33",
        "@types/archy": "^0.0.30",
        "@types/async": "^2.0.40",
        "@types/axios": "^0.14.0",
        "@types/bookshelf": "^0.9.2",
        "@types/commander": "^2.9.0",
        "@types/dotenv": "^4.0.0",
        "@types/faker": "^4.1.0",
        "@types/fs-extra": "^3.0.0",
        "@types/globule": "^1.1.1",
        "@types/gulp": "^3.8.32",
        "@types/gulp-util": "^3.0.31",
        "@types/inquirer": "^0.0.32",
        "@types/knex": "0.0.53",
        "@types/lokijs": "^1.2.30",
        "@types/moment": "^2.13.0",
        "@types/node": "^7.0.8",
        "@types/npm": "^2.0.28",
        "@types/raven": "^1.2.4",
        "@types/reflect-metadata": "^0.0.5",
        "@types/shelljs": "^0.7.1",
        "@types/sinon": "^2.3.1",
        "@types/string": "^0.0.28",
        "@types/table": "^4.0.2",
        "@types/through2": "^2.0.32",
        "@types/tmp": "^0.0.33",
        "@types/vinyl": "^2.0.0",
        "@types/vinyl-fs": "^2.4.5",
        "@types/winston": "^2.3.0",
        "bluebird": "^3.4.1",
        "browserify": "^13.0.0",
        "cli-clear": "^1.0.4",
        "connect": "^3.5.0",
        "copy": "^0.3.0",
        "del": "^3.0.0",
        "download": "^6.2.5",
        "es6-symbol": "^3.1.0",
        "globule": "^1.0.0",
        "gulp": "^3.9.1",
        "gulp-clean": "^0.3.2",
        "gulp-gh-pages": "^0.5.4",
        "gulp-header": "1.8.8",
        "gulp-mocha": "^3.0.0",
        "gulp-rename": "^1.2.2",
        "gulp-rollup": "^2.5.1",
        "gulp-sourcemaps": "^1.6.0",
        "gulp-tsd": "0.1.1",
        "gulp-tslint": "^6.0.1",
        "gulp-typescript": "^3.0.0",
        "gulp-uglify": "^2.0.0",
        "gulp-util": "^3.0.8",
        "harmonize": "^1.4.4",
        "harmony-proxy": "^1.0.0",
        "inversify-tracer": "^1.0.8",
        "inversify-vanillajs-helpers": "^2.0.0",
        "lodash": "4.0.0",
        "mocha-typescript": "^1.1.7",
        "mocha": "^3.0.1",
        "run-sequence": "^1.2.2",
        "sinon": "^2.3.4",
        "@types/chai": "^4.0.0",
        "chai": "^3.5.0",
        "npm": "^5.1.0",
        "publish-please": "^2.1.4",
        "rollup": "^0.35.12",
        "rollup-plugin-typescript": "^0.8.1",
        "rollup-plugin-uglify": "^1.0.1",
        "table": "^4.0.1",
        "through2": "^2.0.3",
        "ts-node": "^3.2.0",
        "tsd": "0.5.6",
        "tslint": "^3.15.1",
        "typedoc": "^0.5.10",
        "typescript": "^2.4.2",
        "validatorjs": "^3.12.0",
        "vinyl-buffer": "^1.0.0",
        "vinyl-source-stream": "^1.1.0"
    }
}
